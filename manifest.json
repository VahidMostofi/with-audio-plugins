{

  "manifest_version": 2,
  "name": "Borderify",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["assets/js/jquery-3.6.0.min.js","borderify.js"]
    }
  ],
  "permissions": [
    "*://localhost/*"
  ]

}
